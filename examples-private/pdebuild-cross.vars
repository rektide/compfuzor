---
ARCH: armel
noauth: true
REPOS:
- name: debian
  source: http://debian.gtisc.gatech.edu/debian/
  suite: jessie
  keyring: debian-keyring
  pkg_sets:
  - "{{BASE}}"
  - "{{hostvars[inventory_hostname]['BASE_'+ARCH}}"
  - "{{AVAHI}}"
  pkgs:
  - pwgen
  - screen
  - usbip
  components:
  - main
  - contrib
  - non-free
